<?xml version="1.0" encoding="utf-8"?>
<template xml:space="preserve">
	<div t-name="OperationalDashboard">
		<div class="container">
			<div class="col-md-12 atas filter">
				<div class="panel panel-g col-md-4">
					<div class="panel-header">
						<h3>Filter Project</h3>
					</div>
					<div class="panel-body">
						<select id="filter_project" class="form-control">
							<option value="active">Aktif Project</option>
							<!-- <option value="all">Semua Project</option> -->
						</select>
					</div>
				</div>
				<div class="panel panel-g col-md-4">
					<div class="panel-header">
						<h3>Tahun</h3>
					</div>
					<div class="panel-body">
						<select id="filter_tahun" class="form-control">
							<option value="2018">2018</option>
							<option value="2017">2017</option>
							<option value="2016">2016</option>
						</select>
					</div>
				</div>
				<div class="panel panel-g col-md-4">
					<div class="panel-header">
						<h3>Bulan</h3>
					</div>
					<div class="panel-body">
						<select id="filter_bulan" class="form-control">
							<option selected="selected" id="month_def" value="">-- Pilih Bulan --</option>
							<option value="1">Jan</option>
							<option value="2">Feb</option>
							<option value="3">Mar</option>
							<option value="4">Apr</option>
							<option value="5">Mei</option>
							<option value="6">Jun</option>
							<option value="7">Jul</option>
							<option value="8">Agu</option>
							<option value="9">Sep</option>
							<option value="10">Okt</option>
							<option value="11">Nov</option>
							<option value="12">Des</option>
						</select>
					</div>
				</div>
			</div>

			<div id="gantt_here" style='width:200%;'></div>
			<div id="pivotgrid" style="margin-top: 150px; background: white; margin-bottom: 50px;"></div>
		</div>
	</div>
</template>
